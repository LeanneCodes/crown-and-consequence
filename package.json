{
  "name": "crown-and-consequence",
  "version": "1.0.0",
  "description": "A decision-based game",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -L ./server/index.js",
    "start": "node ./server/index.js",
    "setup-db": "node ./server/db/setup.js",
    "test": "jest",
    "test:backend": "jest --selectProjects backend",
    "test:frontend": "jest --selectProjects frontend",
    "testw": "jest --watchAll --detectOpenHandles --runInBand --verbose",
    "unitTests": "jest --testPathPattern=/unit/ --watchAll --verbose",
    "integrationTests": "jest --testPathPattern=/integration/ --watchAll --detectOpenHandles --runInBand --verbose",
    "coverage": "jest --coverage --testPathPattern=/unit/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeanneCodes/crown-and-consequence.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/LeanneCodes/crown-and-consequence/issues"
  },
  "homepage": "https://github.com/LeanneCodes/crown-and-consequence#readme",
  "devDependencies": {
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^21.1.2",
    "nodemon": "^3.1.11",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "node": "^24.12.0",
    "path": "^0.12.7",
    "pg": "^8.16.3",
    "supabase": "^2.67.1"
  }
}
